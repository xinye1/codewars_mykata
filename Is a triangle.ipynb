    "def is_triangle(a, b, c):\n",
    "    tmp_ls = [a, b, c]\n",
    "    for x in [a, b, c]:\n",
    "        tmp_ls.remove(x)\n",
    "        if x > sum(tmp_ls):\n",
    "            return False\n",
    "        tmp_ls.append(x)\n",
    "    return True\n",
    "is_triangle(7, 2, 2)"
   ]
